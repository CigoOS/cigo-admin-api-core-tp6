{
  "name": "cigoos/cigo-admin-api-core-tp6",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "西谷老湿",
      "email": "zim@cigoos.com"
    }
  ],
  "keywords": [
    "framework",
    "cigoadmin"
  ],
  "homepage": "https://www.cigoadmin.com",
  "description": "西谷后台-后端接口核心库:thinkphp-v6版本",
  "minimum-stability": "dev",
  "require": {
    "alibabacloud/client": "^1.5",
    "ext-json": "^7.4",
    "ext-curl": "^7.4",
    "ext-openssl": "^7.4",
    "ext-mbstring": "^7.4",
    "overtrue/wechat": "^5.0",
    "php": ">=7.1.0",
    "qiniu/php-sdk": "^7.2",
    "topthink/framework": "^6.0.0",
    "topthink/think-orm": "^2.0",
    "topthink/think-image": "^1.0",
    "topthink/think-view": "^1.0",
    "yansongda/pay": "^2.9"
  },
  "require-dev": {
    "symfony/var-dumper": "^5.2",
    "topthink/think-trace": "^1.4"
  },
  "autoload": {
    "files": [],
    "psr-4": {
      "cigoadmin\\": "src/"
    }
  },
  "extra": {
    "think": {
      "services": [
        "cigoadmin\\Service"
      ],
      "config": {
        "app": "src/config/app.php",
        "cigoadmin": "src/config/cigoadmin.php"
      }
    }
  }
}
